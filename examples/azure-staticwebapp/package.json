    "@remix-run/react": "^2.17.2",
    "@remix-run/serve": "^2.17.2",
    "@scandinavianairlines/remix-azure-functions": "^2.0.1",
    "isbot": "^5.1.32",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@azure/static-web-apps-cli": "^2.0.7",
    "@remix-run/dev": "^2.17.2",
    "@remix-run/eslint-config": "^2.17.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "azure-functions-core-tools": "^4.4.0",
    "eslint": "^9.39.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3"
  },
